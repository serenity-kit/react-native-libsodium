{
  "name": "react-native-libsodium",
  "version": "1.4.0",
  "description": "React Native bindings to Libsodium matching the libsodium-wrappers API",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "libsodium",
    "!libsodium/build",
    "app.plugin.js",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "test:e2e:web": "playwright test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build",
    "postinstall": "tar -xzf libsodium/build.tgz --directory ./libsodium"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/serenity-kit/react-native-libsodium",
  "author": "Nik Graf <nik@nikgraf.com> (https://github.com/nikgraf)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serenity-kit/react-native-libsodium/issues"
  },
  "homepage": "https://github.com/serenity-kit/react-native-libsodium#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@expo/config-plugins": "^7.2.5",
    "@playwright/test": "^1.51.1",
    "@react-native/eslint-config": "0.79.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.6.3",
    "pod-install": "^0.1.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-builder-bob": "^0.40.5",
    "release-it": "^15.0.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.21",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@noble/hashes": "^1.3.2",
    "@types/libsodium-wrappers": "^0.7.13",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "libsodium-wrappers": "^0.7.13",
    "libsodium-wrappers-sumo": "^0.7.13"
  }
}
