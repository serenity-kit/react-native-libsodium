{
  "name": "react-native-libsodium",
  "version": "1.6.0",
  "description": "React Native bindings to Libsodium matching the libsodium-wrappers API",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "libsodium",
    "!libsodium/build",
    "app.plugin.js",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "test:e2e:web": "playwright test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build",
    "postinstall": "tar -xzf libsodium/build.tgz --directory ./libsodium"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/serenity-kit/react-native-libsodium",
  "author": "Nik Graf <nik@nikgraf.com> (https://github.com/nikgraf)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serenity-kit/react-native-libsodium/issues"
  },
  "homepage": "https://github.com/serenity-kit/react-native-libsodium#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.4.1",
    "@evilmartians/lefthook": "^2.1.0",
    "@expo/config-plugins": "^7.2.5",
    "@playwright/test": "^1.58.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/jest": "^30.0.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "commitlint": "^20.4.1",
    "del-cli": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "pod-install": "^1.0.9",
    "prettier": "^3.8.1",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-builder-bob": "^0.40.17",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "yarn@1.22.21",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@noble/hashes": "^1.3.2",
    "libsodium-wrappers": "^0.8.2",
    "libsodium-wrappers-sumo": "^0.8.2"
  }
}
